<h1>Step 4. How can we contact you?</h1>

<%= fp.fields_for :contact do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>

  <div>
    <%= f.label :phone %>
    <%= f.text_field :phone %>
  </div>

  <div>
    <%= f.label :notes %>
    <%= f.text_field :notes %>
  </div>
<%end%>