      <% p1 = @properties_p.find_by(list_price_cents: 5888800000)%>
        <div class="small-12 medium-12 large-6 columns">
          <div class="listing-item">
          <%= link_to "", property_path(p1.id), class: "listing-link" %>
          <%# @properties_p.first.photos.each do |p|%>
            <div class="listing-img" data-bg=<%= @properties_p.find_by(list_price_cents: 5888800000).photos[1].picture_url %>>
          <%# end %>
              <div class="listing-badge">
              <%if p1.list_price_cents != 0%>
                <span>For Sale</span>
              <% else %>
                <span class="sold">Sold</span>
              <% end %>
              </div>
              
              <% if p1.list_price_cents > 0 %>
              <div class="listing-price">
                <sup>$</sup>
                <span style=""><%= p1.price %></span>
              </div>
               <% end %>
            </div>
            <div class="listing-meta clearfix">
              <div>
                <p><%= p1.address.address_first %></p>
                <span style="font-size: 0.90rem; display: block; font-weight: 200;" class="dark-grey"><%= p1.address.city %>, BC</span>
              </div>
              <div class=" row attributes">
                <p class=" small-2 medium-2 columns"><small><strong><%= p1.bedrooms %></strong> Beds</small></p>

                <p class="small-3 medium-2 columns"><small><strong><%= p1.bathrooms %></strong> Bath</small></p>

                <p class="small-4 medium-3 columns"><small><strong><%= p1.floor_area%></strong> Sq.Ft.</small></p>

                <p class="small-1 medium-2 end columns"><small>Yr.<strong><%= p1.year_built%></strong></small></p>
              </div>
            </div>
          </div>
        </div>