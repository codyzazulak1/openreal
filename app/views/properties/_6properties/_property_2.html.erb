        <% p2 = @feature2_property%>
        <div class="small-12 medium-12 large-6 columns">
          <div class="listing-item">
          <%= link_to "", property_path(p2.id), class: "listing-link" %>
          <%# @properties_p.second.photos.each do |p|%>
          <% if p2.featured_photo.blank? || p2.featured_photo.nil?%>
            <div class="listing-img" data-bg=<%= p2.photos.first.picture_url %>>
          <% else %>
            <div class="listing-img" data-bg=<%= p2.featured_photo %>>
          <% end %>
          <%# end %>
              <div class="listing-badge">
              <%if p2.list_price_cents != 0%>
                <span>For Sale</span>
              <% else %>
                <span class="sold">Sold</span>
              <% end %>
              </div>
              
              <% if p2.list_price_cents > 0 %>
              <div class="listing-price">
                <sup>$</sup>
                <span style=""><%= p2.price %></span>
              </div>
               <% end %>
            </div>
            <div class="listing-meta clearfix">
              <div>
                <p><%= p2.address.address_first %></p>
                <span style="font-size: 0.90rem; display: block; font-weight: 200;" class="dark-grey"><%= p2.address.city %>, BC</span>
              </div>
              <div class=" row attributes">
                <p class=" small-2 medium-2 columns"><small><strong><%= p2.bedrooms %></strong> Beds</small></p>

                <p class="small-3 medium-2 columns"><small><strong><%= p2.bathrooms %></strong> Bath</small></p>

                <p class="small-4 medium-3 columns"><small><strong><%= p2.floor_area%></strong> Sq.Ft.</small></p>

                <p class="small-1 medium-2 end columns"><small>Yr.<strong><%= p2.year_built%></strong></small></p>
              </div>
            </div>
          </div>
        </div>
