<!--hero-->
<section class="" id="hero">
  <div class="hero-bg-overlay"></div>
  <div class="hero-bg"></div>
  <div class="intro">
    <div class="row expanded">
      <div class="small-9 medium-8 medium-push-2 columns">
        <h1>Welcome to OpenReal</h1>
        <p>Sell your home to OpenReal and eliminate the hassle of listings, showings and months of uncertainty.</p>
      </div>
      <!--Search Bar-->
      <div class="search-bar">
        <div class="small-9 medium-8 medium-push-2 columns">
          <!-- Search field -->
          <%= form_tag (properties_sell_path), id: "addressForm" do %>
            <div>
              <input type="text" id="addressInput" placeholder="Enter your home address" name="addressInput">
              <input type="hidden" name="addressFirst" id="addressFirst">
              <input type="hidden" name="addressSecond" id="addressSecond">
              <input type="hidden" name="addressCity" id="addressCity">
              <input type="hidden" name="addressPostal" id="addressPostal">
              <input type="hidden" name="addressLat" id="addressLat">
              <input type="hidden" name="addressLng" id="addressLng">
              <input type="submit" class="hide">
              <!-- <div class="input-group-button">
                <button class="button">Get&nbsp;Offer</button>
              </div> -->
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</section>
<!--end of hero-->

<!-- listing banner -->
<section class="listing-banner row expanded" id="listing-banner">
  <div class="medium-10 medium-centered columns">
    <div class="row collapse">
      <!-- photo carousel -->
      <div class="medium-8 columns">
        <div class="orbit photo-carousel" role="region" data-orbit>
          <ul class="orbit-container">
            <li class="orbit-slide">
              <div class="photo-slide" data-bg=<%= asset_path('P1.jpg') %>></div>
            </li>
            <li class="orbit-slide">
              <div class="photo-slide" data-bg=<%= asset_path('P5.jpg') %>></div>
            </li>
            <li class="orbit-slide">
              <div class="photo-slide" data-bg=<%= asset_path('P4.jpg') %>></div>
            </li>
            <li class="orbit-slide">
              <div class="photo-slide" data-bg=<%= asset_path('P5.jpg') %>></div>
            </li>
          </ul>
        </div>
      </div>
      <!-- CTA -->
      <div class="medium-4 columns cta text-center">
        <div class="v-align">
          <h2>Buy an OpenReal Home that youâ€™re guaranteed to love.</h2>
          <a href="#" class="btn btn-primary">View our homes</a>
        </div>
      </div>

  </div>
</section>
<!-- end of listing banner -->

<!-- why -->
<section class="row expanded" id="why">
  <div class="row text-center section-title">
    <h2 class="medium-6 medium-push-3 columns">With OpenReal, your home buying and selling experience is hassle-free.</h2>
  </div>
  <div class="row collapse">
    <div class="medium-10 medium-centered columns">
      <div class="row">
        <div class="small-12 medium-4 large-4 columns">
          <div class="points">
            <%= image_tag('bubble.svg') %>
            <p class="lead">Selling your home hassle-free the moment when you are ready</p>
            <p class="subheader">No need to hire an agent, list your home and wait for months for transaction to complete.</p>
          </div>
        </div>
        <div class="small-12 medium-4 large-4 columns">
          <div class="points">
            <%= image_tag('data-house.svg') %>

            <p class="lead">Obtaining a fair offer by telling us everything about your home</p>
            <p class="subheader">No need to host countless showings with contracts falling through.</p>
          </div>
        </div>
        <div class="small-12 medium-4 large-4 columns">
          <div class="points">
            <%= image_tag('truck.svg') %>
            <p class="lead">Moving in to your dream home worry-free</p>
            <p class="subheader">All of our listings have certified home inspection and extended coverage warranty.</p>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="row text-center section-title">
    <a href="#" class="btn btn-secondary">Learn more</a>
  </div>
</section>
<!-- end of why -->















