<div class="agent-form">
  <%= form_for aform do |f| %>
    <div class="field">
      <%= f.label :full_name %>
      <%= f.text_field :full_name %>
    </div>
    <div class="field">
      <%= f.label :email %>
      <%= f.text_field :email %>
    </div>
    <div class="field">
      <%= f.label :company_name %>
 
      <%= f.select :company_name, options_for_select(["Sutton", "Remax", "Century21","Royal Pacific", "Rennie Group", "MacDonald Westmar", "other"]), {:prompt => true}%>

    </div>
    <div class="field agent-submit">
      <%= f.submit "Submit" %>
    </div>
  <%end%>
</div>
