<h1>Edit this home</h1>
<h5><%= @property.address_name %></h5>
<button id="edit-address">Edit Address</button>
<button id="edit-property" hidden>Edit Property</button>

<%= form_for @property, html: {id: "property-form"} do |f| %>
  <div id="edit-property-1">
    <% @property_attributes.each do |u| %>
      <%= f.label u %>
      <%= f.text_field u.to_sym %>
    <% end %>
    <%= f.submit "Save" %>
  </div>
  <div id="edit-property-2">
    <%= fields_for :address do |fa| %>
      <% @address_attributes.each do |a| %>
        <%= fa.label a %>
        <%= fa.text_field a.to_sym %>
      <% end %>
    <% end %>
    <%= f.submit "Save" %>
  </div>
<%end%>
