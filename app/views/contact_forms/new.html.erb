<h1>Contact Us</h1>
<%= form_for @cform do |f| %>
  <div>
    <%= f.label "Your Name" %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label "Your Email" %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label "Your Phone Number" %>
    <%= f.text_field :phone %>
  </div>
  <div>
    <%= f.label "Your Messages" %>
    <%= f.text_area :notes %>
  </div>
  <div>
    <%= f.submit "Done" %>
  </div>
<% end %>
