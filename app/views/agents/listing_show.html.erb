<div class="row expanded push-footer" style="margin-top: 70px">
	<%= render :partial => 'dashboard/side_panel' %>

	<div class="medium-9 columns dashboard-content" id="agent-listings-dash">
		<h1><%= @property.address_name %></h1>
		<hr>
		<br>
		<div class="dashboard-list">
			<div class="dashboard-list-header row collapse">
				<div class="small-12 columns" style="padding-bottom: 1em;">

					<div class="float-left">
					<a href="<%= agent_listings_path%>" class="btn btn-secondary">Back</a>
					</div>

					 <div class="float-right">
					 <%= link_to 'Edit', agent_listing_edit_path(@property), class: "btn btn-secondary"%>
					 <%= link_to 'Delete', agent_listing_path(@property.id), data: {confirm: 'Are you sure?'}, method: :delete, class: 'btn btn-secondary'%>
					 </div>

				</div>
			</div>

			<div class="dashboard-list-body">
				<div class="row">

					<div class="small-6 columns">
						<ul>
							<% @address_attributes.each do |u| %>
							<li>
								<h5> <%= u%></h5>
								<p><%= @property.address[u] %></p>
							</li>
							<% end %>
						</ul>
					</div>

					<div class="small-6 columns">
            <ul style="list-style-type: circle;">
              <% @property_attributes.each do |u| %>
              <% if (u != 'agent_id')%>
              <% if (u != 'status_id')%>
              <% if (u != 'sold')%>
                <li>
                  <h5><%= u %> :</h5>

                  <p><%= !(@property[u].blank?) ? @property[u] : 'Not set' %></p>
                </li>
              <% end %>
              <% end %>
              <% end %>
              <%end%>
            </ul>
					</div>

				</div>
			</div>
		</div>

	</div>
</div>