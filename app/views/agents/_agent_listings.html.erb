<% @properties.each do |p|%>
<li class="dashboard-list-item row collapse">

	<div class="small-1 columns dashboard-listing-img">
		<a href="<%= agent_listing_show_path(p.id)%>">
			<img src="<%= p.photos.count == 0 ? 'http://placehold.it/350x150?text=%20' : p.featured_photo %>">
		</a>
	</div>

	<div class="small-6 columns end">
		<div class="dashboard-listing-overview float-left">
			<h3>
				<% if p.address%>
					<%= link_to p.address_name, agent_listing_show_path(p.id)%>
				<% else %>
				No associated address. (<%= p.created_at%>)
				<% end %>
			</h3>
			<div>
				<% puts "id: #{p.id}"%>
				<%= link_to 'Edit', agent_listing_show_path(p.id)%> | <%= link_to 'Delete', agent_listing_path(p.id), data: {confirm: 'Are you sure?'}, method: :delete %>
			</div>
		</div>
	</div>

	<div class="small-4 columns" id="status-prop-agents">
		<% if p.status_id === 4%>
		<p>Status:  <span class="submitted">Submitted</span></p>
		<% elsif p.status_id == 5%>
		<p>Status:  <span class="live">Live</span></p>
		<% end %>
	</div>
</li>
<% end %>